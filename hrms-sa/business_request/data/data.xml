       <record id="seq_expense_claim_request" model="ir.sequence">
         <field name="name">Expense Claim Request</field>
         <field name="code">expense.claim</field>
         <field name="prefix">EC</field>
         <field name="padding">3</field>
         <field eval="False" name="company_id"/>
      </record>
</data>
</odoo>